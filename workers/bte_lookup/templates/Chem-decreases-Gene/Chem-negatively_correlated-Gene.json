{
  "query_graph": {
    "nodes": {
      "$source": {
        "ids": ["$source_id"],
        "categories": ["biolink:ChemicalEntity"]
      },
      "$target": {
        "ids": ["$target_id"],
        "categories": ["biolink:Gene", "biolink:Protein"]
      }
    },
    "edges": {
      "eA": {
        "subject": "$source",
        "object": "$target",
        "predicates": ["biolink:negatively_correlated_with"]
      }
    }
  }
}
