{
  "query_graph": {
    "nodes": {
      "$source": {
        "ids": ["$source_id"],
        "categories": ["biolink:Treatment"]
      },
      "$target": {
        "ids": ["$target_id"],
        "categories": ["biolink:DiseaseOrPhenotypicFeature"]
      }
    },
    "edges": {
      "eA": {
        "subject": "$source",
        "object": "$target",
        "predicates": ["biolink:related_to"]
      }
    }
  }
}
