{
  "query_graph": {
    "nodes": {
      "$source": {
        "ids": ["$source_id"],
        "categories": [
          "biolink:Procedure",
          "biolink:Treatment",
          "biolink:ClinicalIntervention"
        ]
      },
      "$target": {
        "ids": ["$target_id"],
        "categories": ["biolink:DiseaseOrPhenotypicFeature"]
      }
    },
    "edges": {
      "eA": {
        "subject": "$source",
        "object": "$target",
        "predicates": ["biolink:treats"]
      }
    }
  }
}
