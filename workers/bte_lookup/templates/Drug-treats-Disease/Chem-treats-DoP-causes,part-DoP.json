{
  "query_graph": {
    "nodes": {
      "$source": {
        "ids": ["$source_id"],
        "categories": ["biolink:ChemicalEntity"]
      },
      "nA": {
        "categories": ["biolink:DiseaseOrPhenotypicFeature"],
        "is_set": true
      },
      "$target": {
        "ids": ["$target_id"],
        "categories": ["biolink:DiseaseOrPhenotypicFeature"]
      }
    },
    "edges": {
      "eA": {
        "subject": "$source",
        "object": "nA",
        "predicates": ["biolink:treats"]
      },
      "eB": {
        "subject": "nA",
        "object": "$target",
        "predicates": ["biolink:part_of", "biolink:causes"]
      }
    }
  }
}
